{% extends '::base.html.twig' %}
{% block body %}
<div align="center"><h1>LISTADO DE PROFESORES</h1></div>
<div align="center">
  <table>
    <tr>
      <th>#</th>
      <th>NOMBRE</th>
      <th>APELLIDOS</th>
      <th>DEPARTAMENTO</th>
    </tr>
    {% for profesor in profesores %}
    <tr>
      <td>{{ profesor.id }}</td>    
      <td>{{ profesor.nombre }}</td>
      <td>{{ profesor.apellidos }}</td>
      <td>{{ profesor.departamento }}</td>
    </tr>
    {% endfor %}
  </table>
</div>
{% endblock %}
