{% extends '::base.html.twig' %}
{% block body %}
<div align="center"><h1>CONFIGURACIONES</h1></div>
<div align="center">
  <table>
    <tr>
      <th>#</th>
      <th>PARAM</th>
      <th>CONFIG</th>
    </tr>
    {% for config in configs %}
    <tr>
      <td>{{ config.id }}</td>
      <td>{{ config.param }}</td>
      <td>{{ config.config }}</td>
    </tr>
    {% endfor %}
  </table>
</div>
{% endblock %}
